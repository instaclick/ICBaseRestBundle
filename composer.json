{
    "name": "instaclick/base-rest-bundle",
    "description": "This bundle provides REST support for Dating Engine",
    "keywords": ["base", "rest", "api"],
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "InstaClick Inc.",
            "homepage": "http://www.instaclick.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "http://satis.dev:8080/"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/symfony": "~2.2"
    },
    "require-dev": {
        "phpunit/PHPUnit": "~3.7"
    },
    "autoload": {
        "psr-0": { "IC\\Bundle\\Base\\RestBundle": "" }
    },
    "target-dir": "IC/Bundle/Base/RestBundle",
    "minimum-stability": "dev"
}
